{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        },
        "logLevel": {
            "default": "Information"
        }
    },
    "customHandler": {
        "description": {
            "defaultExecutablePath": "node",
            "defaultWorkerPath": "index.mjs"
        },
        "enableForwardingHttpRequest": true
    },
    "extensions": {
        "http": {
            "routePrefix": ""
        }
    }
}