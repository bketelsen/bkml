{
  "functions": {
    "graphql/index.js": {
      "maxDuration": 5
    },
    "rest/index.js": {
      "maxDuration": 5
    }
  },
  "rewrites": [
    {
      "source": "/graphql/(.*)",
      "destination": "/graphql"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ]
}
